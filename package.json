{
  "name": "minnie",
  "version": "0.8.2",
  "description": "A WordPress front-end built with React.js",
  "author": "Dzikri Aziz",
  "repository": "https://github.com/kucrut/minnie",
  "main": "index.js",
  "license": "GPL-2.0",
  "scripts": {
    "lint:js": "eslint webpack server app --ext .js,.jsx",
    "lint": "npm run lint:js",
    "clean": "rimraf public",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "dev": "npm run clean && cross-env NODE_ENV=development nodemon",
    "build:dev": "cross-env NODE_ENV=development webpack --colors --config ./webpack/webpack.config.dev-server.js",
    "build:prod": "cross-env NODE_ENV=production webpack --colors --config ./webpack/webpack.config.prod.js",
    "build": "npm run clean && npm run build:prod"
  },
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/runtime-corejs2": "^7.1.2",
    "axios": "^0.14.0",
    "classnames": "^2.2.5",
    "cross-env": "^2.0.1",
    "css-loader": "^1.0.1",
    "dom-closest": "^0.2.0",
    "es6-promise": "^3.2.1",
    "express": "^4.14.0",
    "file-loader": "^2.0.0",
    "he": "^1.1.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.15.0",
    "photoswipe": "^4.1.1",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-mixins": "^6.2.0",
    "postcss-nested": "^4.1.0",
    "postcss-reporter": "^6.0.0",
    "postcss-simple-vars": "^5.0.1",
    "prismjs": "^1.5.1",
    "qs": "^6.2.1",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.1.0",
    "react-router": "^4.3.1",
    "react-router-redux": "^4.0.5",
    "react-transform-hmr": "^1.0.4",
    "redux": "^4.0.1",
    "redux-thunk": "^2.1.0",
    "rimraf": "^2.5.4",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.23.1"
  },
  "devDependencies": {
    "nodemon": "^1.10.2"
  }
