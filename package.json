{
  "name": "minnie",
  "version": "0.1.0",
  "description": "A WordPress front-end built with React.js",
  "author": "Dzikri Aziz",
  "repository": "https://bitbucket.org/kucrut/wp-minnie",
  "main": "index.js",
  "license": "GPL-2.0",
  "scripts": {
    "lint": "eslint app",
    "clean": "rimraf public",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build:dev": "cross-env NODE_ENV=development webpack --colors --config ./webpack/webpack.config.dev-server.js",
    "build:prod": "cross-env NODE_ENV=production webpack --colors --config ./webpack/webpack.config.prod.js",
    "build": "npm run clean && npm run build:prod",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "axios": "^0.9.1",
    "babel": "^6.5.2",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-react-constant-elements": "^6.5.0",
    "babel-plugin-transform-react-inline-elements": "^6.6.5",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "classnames": "^2.2.3",
    "cross-env": "^1.0.7",
    "es6-promise": "^3.1.2",
    "eslint": "^2.7.0",
    "eslint-plugin-react": "^4.2.3",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "immutable": "^3.7.6",
    "inline-environment-variables-webpack-plugin": "0.0.2",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.4",
    "lodash": "^4.8.2",
    "postcss-import": "^8.1.0",
    "postcss-loader": "^0.8.2",
    "postcss-mixins": "^4.0.1",
    "postcss-nested": "^1.0.0",
    "postcss-reporter": "^1.3.3",
    "postcss-simple-vars": "^1.2.0",
    "react": "^0.14.8",
    "react-dom": "^0.14.8",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "react-router-redux": "^4.0.1",
    "react-transform-hmr": "^1.0.4",
    "redux": "^3.3.1",
    "redux-devtools": "^3.2.0",
    "redux-thunk": "^2.0.1",
    "rimraf": "^2.5.2",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
